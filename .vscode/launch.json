    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "name": "Pytest",
            "type": "python",
            "request": "launch",
            "module": "pytest"
        },
        {
            "name": "Equator: Run Interpreter",
            "type": "python",
            "request": "launch",
            "module": "equator",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        }
    ]
}
